{
  "name": "atom-mmb-scrollwheel",
  "main": "./index.js",
  "version": "0.0.1",
  "description": "Middle Mouse Button Scrolling in atom",
  "keywords": [
    "middle",
    "click",
    "scroll",
	"scrolling",
	"middle mouse button scroll",
	"mmb",
	"middle mouse button",
    "editor"
  ],
  "repository": "https://github.com/gcwngo/atom-mmb-scrollwheel",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0"
  },
  "configSchema": {
    "speed": {
      "type": "integer",
      "default": 5,
      "minimum": 1,
      "title": "Speed",
      "description": "The scrolling speed (higher is slower)."
    },
    "threshold": {
      "type": "integer",
      "default": 15,
      "minimum": 1,
      "title": "Threshold",
      "description": "The number of pixels the mouse has to be away from the origin in order to move."
    }
  }
}
