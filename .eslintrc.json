{
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"impliedStrict": true,
			"experimentalObjectRestSpread": true
		}
	},
	"globals": {
		"atom": false
	},
	"rules": {
		"semi": 2,
		"block-scoped-var": 2,
		"default-case": 2,
		"no-undefined": 2,
		"no-inline-comments": 2,
		"no-unused-vars": 2,
		"no-undef": 2,
		"eqeqeq": 2,
		"no-warning-comments": 1,
		"keyword-spacing": 1,
		"valid-jsdoc": 1,
		"spaced-comment": 1,
		"no-else-return": 1,
		"strict": 1,
		"quotes": 1,
		"max-len": 0
	}
}
